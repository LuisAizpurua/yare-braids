{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@images/*": ["./src/assets/images/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "esModuleInterop": true,
    "types": ["astro/client"]
  }
}
