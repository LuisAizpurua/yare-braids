{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@images/*": ["./src/assets/images/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
